 <div class="middle-container4">
  <p>_____________________________________________________________________________________________________________________________________________________________________________________________</p>
  <h2 class="text-design data">Doctor Information</h2>

  <!-- Main Site Here -->

  <main id="site-main">
      <div class="container1">
          <form action="/dashboard" method="post">
              <table class="table1">
                  <thead class="thead-dark">
                    <tr>
                      <th>ID Number</th>
                      <th>Name</th>
                      <th>Role</th>
                      <th>Qualification</th>
                      <th>Mobile No.</th>
                      <th>Email</th>
                  </tr>
              </thead>
              <tbody>
                  <% for(var i=0; i < uses.length; i++){ %>            
                    <tr>
                        <td><%= i + 1 %></td>
                        <td><%= uses[i].name %></td>
                        <td><%= uses[i].role %></td>
                        <td><%= uses[i].qualification %></td>
                        <td><%= uses[i].mobileNo %></td>
                        <td><%= uses[i].email %></td>
                    </tr>
                  <% } %>
                  </tbody>
              </table>
          </form>
      </div>
  </main>
  
  
    
