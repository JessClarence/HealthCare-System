<div class="middle-container4">
    <p>_____________________________________________________________________________________________________________________________________________________________________________________________</p>
    <h2 class="text-design data">MEMBERS</h2>

    <!-- Main Site Here -->

    <main id="site-main">
        <div class="container1">
            <form action="/doctor-view-user" method="post">
                <table class="table1">
                    <thead class="thead-dark">
                        <tr>
                            <th>ID Number</th>
                            <th>Name</th>
                            <th>Mobile No.</th>
                            <th>Email</th>
                            <th>Accept</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0; i < use.length; i++){ %>
                        <tr>
                            <td><%= i + 1 %></td>
                            <td><%= use[i].name %></td>
                            <td><%= use[i].mobileNo %></td>
                            <td><%= use[i].email %></td>
                            <td>
                                <a href="#" class="btn border-shadow update">
                                    <span class="text-gradient">
                                        <i class="fas fa-check"></i>
                                    </span>
                                </a>
                                <a  class="btn border-shadow delete" data-id=<%=use[i]._id%>>
                                    <span class="text-gradient">
                                        <i class="fas fa-times"></i>
                                    </span>
                                </a>
                            </td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </form>
        </div>
    </main>
    
    
      
 </div>


